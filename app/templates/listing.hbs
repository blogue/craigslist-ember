<div class="row">
  <div class="col-md-6">
    <h3>{{model.listing.title}}</h3>
    <h5>Listed By: {{model.listing.author}} - {{model.listing.date}}</h5>
    <h5>{{model.listing.content}}</h5>
    <img class="small-img" src="{{model.listing.image}}"/>
  </div>
  <div class="col-md-6">
    <iframe
    width="425"
    height="300"
    frameborder="0" style="border:0"
    src="https://www.google.com/maps/embed/v1/place?key={{model.key}}
    &q={{model.listing.location}}" allowfullscreen>
  </iframe>
  </div>

  {{edit-listing-tile edit='edit' delete='delete' listing=model.listing}}
</div>
{{#link-to 'category' model.listing.category}}Back to Category{{/link-to}}
